{
    "name": "rogercreasy/chassis-php",
    "description": "The ChassisPHP Framework - A Framework for CMS Site Development",
    "keywords": [
        "framework",
        "CMS",
        "Chassis",
        "ChassisPHP"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Roger Creasy",
            "email": "roger@rogercreasy.com",
            "role": "Project Lead"
        }
    ],
    "type": "project",
    "require": {
        "php": "^7.1",
        "symfony/http-foundation": "^3.3",
        "symfony/http-kernel": "^3.3",
        "symfony/var-dumper": "^3.3",
        "league/container": "~2.0",
        "nikic/fast-route": "^1.2",
        "psr/http-message": "^1.0",
        "vlucas/phpdotenv": "^2.4",
        "doctrine/orm": "^2.5",
        "symfony/yaml": "^3.3",
        "monolog/monolog": "^1.23",
        "twig/twig": "^2.0",
        "zendframework/zend-diactoros": "^1.6",
        "symfony/validator": "^3.3"
    },
    "autoload": {
        "files": [
            "lib/Framework/helpers.php"
        ],
        "psr-4": {
            "Lib\\": "lib/",
            "Http\\": "app/Http/",
            "Database\\": "Database/",
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/process": "^4.1"
    },
    "scripts":{
        "backup-sqlite": "\\Database\\Backup::run"
    }
}
